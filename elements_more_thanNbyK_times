//Given an array of size n and a number k, find all elements that appear more than " n/k " times.
#include<bits/stdc++.h>
using namespace std;

void morethanNbyK(int arr[],int n,int k){
    unordered_map<int,int>freq;
    int x = n/k;
    for(int i=0; i<n; i++){
    
        freq[arr[i]]++;
    }

        for(auto i : freq) {     //we have 
        if(i.second > x){       //used for each loop here
    cout<<i.first<<endl ; 
     }
   }
}
//driver code
int main()
{
    int arr[] = { 1, 1, 2, 2, 3, 5, 
                  4, 2, 2, 3, 1, 1, 1 };
    int n = sizeof(arr) / sizeof(arr[0]);
    int k = 4;
      
   morethanNbyK(arr, n, k);
  
    return 0;
}
